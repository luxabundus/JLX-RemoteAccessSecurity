    <IncludePath>$(RasecSourcePath);$(IncludePath)</IncludePath>
    <LibraryPath>$(RasecSourcePath)\RasecCore\Output\$(Configuration);$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup />
